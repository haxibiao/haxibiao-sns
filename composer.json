{
    "name": "haxibiao/sns",
    "description": "哈希表内容社交框架",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "ivan",
            "email": "ivan@haxibiao.com"
        }
    ],
    "require": {
        "php": "^7.3",
        "laravel/framework": "^8.0",
        "haxibiao/console": "*",
        "nuwave/lighthouse": "^4.10",
        "haxibiao/helpers": "*",
        "haxibiao/config": "*"
    },
    "autoload": {
        "psr-4": {
            "Haxibiao\\Sns\\": "src/"
        },
        "classmap": [
            "database/seeders/"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "database/factories/",
            "tests/"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Haxibiao\\Sns\\SnsServiceProvider"
            ]
        }
    }
}
